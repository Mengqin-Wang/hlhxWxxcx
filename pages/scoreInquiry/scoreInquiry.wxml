<!--pages/form2/form2.wxml-->
<form bindsubmit="formSubmit" bindreset="formReset">
	<view class="type">
    <view class="typeName"><text>*</text>类型</view>
    <view class="selectType">
		  <Select my-class="exclass" name="select" prop-array='{{selectArray}}' bind:myget='getDate'></Select>
    </view>
	</view>

  <view class="type">
    <view class="typeName"><text>*</text>姓名</view>
    <view class="selectName">
		   <input placeholder-class="phClass" name="name" class="nameInput" placeholder="请输入您的姓名" />  
    </view>
	</view>  

  <view class="type">
    <view class="typeName"><text>*</text>专业</view>
    <view class="selectName">
		   <input placeholder-class="phClass" name="profession" class="nameInput" placeholder="请输入您报名的专业" />  
    </view>
	</view> 
  
  <view class="type">
    <view class="typeName"><text>*</text>级别</view>
    <view class="selectName">
		   <input placeholder-class="phClass" name="rank" class="nameInput" placeholder="请输入您报名专业相应的级别" />  
    </view>
	</view>  

  <view class="type">
    <view class="typeName"><text>*</text>身份证号码</view>
    <view class="selectName">
		   <input placeholder-class="phClass" name="idCard" class="nameInput" placeholder="请输入您的身份证号码" />  
    </view>
	</view> 

  <view class="type">
    <view class="typeName"><text>*</text>准考证号码</view>
    <view class="selectName">
		   <input placeholder-class="phClass" name="textId" class="nameInput" placeholder="请输入您的准考证号码" />  
    </view>
	</view> 

  <view class="type">
    <view class="typeName"><text>*</text>联系电话</view>
    <view class="selectName">
		   <input placeholder-class="phClass" name="phone" class="nameInput" placeholder="请输入您的联系电话" />  
    </view>
	</view> 

  <view class="type">
    <button class="submitBtn" formType="submit">立即查询</button>
  </view>
    
</form>