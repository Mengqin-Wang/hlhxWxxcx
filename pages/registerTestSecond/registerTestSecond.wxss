/* pages/registerTestSecond/registerTestSecond.wxss */
Page {
  background-color: #F4F4F4;
}

.header {
  width: 100%;
  margin: 20rpx 0;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  height: 120rpx;
}

.item {
  width: 100rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}

.circle {
  width: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 50%;
  color: white;
  background-color: #B9B9B9;
  text-align: center;
  font-size: 36rpx;
  font-family: Roboto;
  font-weight: 400;
}

.text {
  font-size: 22rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  line-height: 34rpx;
  color: rgba(153, 153, 153, 1);
}

.line {
  background-color: #E4E4E4;
  width: 100rpx;
  height: 2rpx;
  margin: 10rpx;
  position: relative;
  top: -15rpx;
}

.active {
  background-color: #FF7F34;
}

.activeText {
  color: #FF7F34;
}

.main {
  width: 92%;
  margin: 50rpx 0;
  padding: 0 4%;
  background-color: white;
}

.Title {
  font-size: 30rpx;
  font-family: Source Han Sans CN;
  font-weight: 500;
  line-height: 110rpx;
  height: 110rpx;
  color: rgba(51, 51, 51, 1);
}

.mianItem{
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  border-top: 2rpx solid #F4F4F4;
}

.ItemName {
  font-size: 26rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: rgba(136, 136, 136, 1);
}

.ItemContainer {
  font-size: 30rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: rgba(53, 53, 53, 1);
  display: flex;
  flex-direction: row;
  align-items: center;
}


.submitBtn {
  /* width: 100%; */
  margin: 50rpx auto;
  height: 90rpx;
  background: linear-gradient(90deg, rgba(250, 153, 51, 1) 0%, rgba(251, 123, 45, 1) 100%);
  opacity: 1;
  font-size: 32rpx;
  border-radius: 45rpx;
  color: white;
  font-family: Source Han Sans CN;
  font-weight: 400;
}

button {
  size: default;
  width: 90% !important;
}
.money{
  color: #FF7F34;
}

.mianItem image{
  
  width: 40rpx;
  height: 40rpx;
}