/* pages/examineeStyle/examineeStyle.wxss */
Page {
  background-color: #F4F4F4;
  padding-top: 40rpx;
}

.type1 {
  width: 94%;
  padding:20rpx 3%;
  background-color: white;
}

.title {
  font-size: 32rpx;
  font-family: Source Han Sans CN;
  font-weight: 500;
  line-height: 60rpx;
  color: rgba(53, 53, 53, 1);
  margin-bottom: 20rpx;
}

.Imgtype1 {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.Imgtype1 image {
  width: 31%;
  height: 160rpx;
  border-radius: 10rpx;
}

.date {
  font-size: 26rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  line-height: 50rpx;
  margin:20rpx 0;
  color: #B2B2B2;
}

.hr{
  width: 100%;
  height: 2rpx;
  background-color: #F4F4F4;
}

.Imgtype3 image{
  width: 97%;
  border-radius: 10rpx;
  height: 280rpx;
}