/* pages/registrationSuccess/registrationSuccess.wxss */
Page {
  background-color: white;
}

.main {
  width: 100%;
  margin-top: 250rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.main image {
  margin: 40rpx 0;
  width: 120rpx;
  height: 120rpx;
}

.success {
  font-size: 40rpx;
  font-family: Source Han Sans CN;
  font-weight: 500;
  line-height: 60rpx;
  color: rgba(51, 51, 51, 1);
}

.btn {
  width: 90%;
  height: 90rpx;
  margin-top: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.btn1 {
  width: 40%;
  height: 90rpx;
  border-radius: 45rpx;
  font-size: 32rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  line-height: 90rpx;
  color: rgba(136, 136, 136, 1);
  text-align: center;
}

.return{
  border: 2rpx solid #B2B2B2;
  color: #888888;
}

.watch{
  border: 2rpx solid #FB7B2D;
  color: #FB7B2D;
}