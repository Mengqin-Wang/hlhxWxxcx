/* pages/scoreInquiryFail/scoreInquiryFail.wxss */
Page {
  background: rgba(0, 0, 0, 0.4);
}

.main {
  width: 80%;
  height: 700rpx;
  margin: 200rpx 10%;
  background-color: white;
  border-radius: 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items:center;
}

.main image {
  width: 120rpx;
  height: 120rpx;
}

.FailTitle {
  margin: 40rpx 0;
  font-size: 40rpx;
  font-family: Source Han Sans CN;
  font-weight: 500;
  line-height: 64rpx;
  color: rgba(51, 51, 51, 1);
}

.FailDetail {
  width: 80%;
  font-size: 32rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  line-height: 48rpx;
  color: rgba(102, 102, 102, 1);
}

.btn{
  width: 80%;
  height: 90rpx;
  border-radius: 45rpx;
  line-height: 90rpx;
  background:rgba(255,127,52,1);
  color: white;
  margin: 40rpx 0;
  text-align: center;
}