<!--pages/teach/teach.wxml-->
<view class="teacher">
	<view wx:for="{{teacherList}}" wx:for-item="item" wx:key="id" data-id="{{item.id}}" class="teacherItem">
		<image src="{{item.image}}"></image>
    <view class="teacherDetail">
      <text class="teacherName">{{item.title}}</text>
      <text class="teacherType">{{item.type}}</text>
    </view>
	</view>
</view>